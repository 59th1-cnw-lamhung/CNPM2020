@model V1_BTL_CNPM.Models.thoigianhoc

@{
    ViewBag.Title = "Delete";
}

<h4>Xóa thông tin thời gian học</h4>
<hr />
<h5 style="color:red">Bạn có muốn xóa thời gian học này không ?</h5>
<div>
    <dl class="dl-horizontal">
        <dd>
            <b>Mã thời gian học</b>: @Html.DisplayFor(model => model.MaTGH)
        </dd>

        <dd>
            <b>Năm học</b>: @Html.DisplayFor(model => model.NamHoc)
        </dd>



        <dd>
            <b>Học kỳ</b>: @Html.DisplayFor(model => model.HocKy)
        </dd>



        <dd>
            <b>Giai đoạn</b>: @Html.DisplayFor(model => model.GiaiDoan)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Xóa bản ghi" class="btn btn-success" /> |
            
        </div>
    }
<div>@Html.ActionLink("Trở lại danh sách", "Index")</div>
</div>
